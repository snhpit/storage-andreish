@using System.Web.UI.WebControls
@model Mvc.Entities.InputInfo
@{
    ViewBag.Title = "Home Page";
}
<h2>@ViewBag.Message</h2>
<div class="centerwrapper">
    @using (Html.BeginForm("Index", "Home"))
    {
        <p>
            @Html.TextBoxFor(model => model.Company, new { id = "quote", placeholder = "Company symbol" })
            @*<input type="text" id="quote" name="Company" placeholder="Company symbol" />*@
            <input type="submit" value="Get quotes" />
        </p>
        <p>
            @*<input type="text" id="from" name="From" placeholder="From" />*@
            @Html.TextBoxFor(model => model.DateFrom, new { id = "from", placeholder = "From" })
            @*<input type="text" id="to" name="To" placeholder="To" />*@
            @Html.TextBoxFor(model => model.DateTo, new { id = "to", placeholder = "To" })
        </p>
        <div id="radio">
            @Html.RadioButtonFor(model => model.Provider, "Google", new { id = "radio1", value = "Google" })
            @Html.Label("radio1", "Google");
            @Html.RadioButtonFor(model => model.Provider, "Yahoo", new { id = "radio2", value = "Yahoo" })
            @Html.Label("radio2", "Yahoo");
            @*<input type="radio" value="Google" id="radio1" name="Provider" /><label name="Provider" for="radio1">Google</label>
            <input type="radio" value="Yahoo" id="radio2" name="Provider" /><label name="Provider" for="radio2">Yahoo</label>*@
        </div>
    }
