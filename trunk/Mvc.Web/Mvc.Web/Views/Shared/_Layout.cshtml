<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>@*<link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />*@
    <link href="@Url.Content("~/Content/themes/base/jquery-ui-1.8.20.custom.css")" rel="stylesheet"
        type="text/css" />
    <link href="@Url.Content("~/Content/web-site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.tmpl.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.20.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/web-site.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.zebra.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jQueryRotate.2.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.tablesorter.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.tablesorter.pager.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.cookie.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
    <script id="tableTemplate" type="text/x-jquery-tmpl">
        <table>
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Open</th>
                    <th>High</th>
                    <th>Low</th>
                    <th>Close</th>
                    <th>Volume</th>
                </tr>
            </thead>
            <tbody>
                {{each values}}
                <tr>
                    <td>${$value['Date']}</td>
                    <td>${$value['Open']}</td>
                    <td>${$value['High']}</td>
                    <td>${$value['Low']}</td>
                    <td>${$value['Close']}</td>
                    <td>${$value['Volume']}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </script>
</head>
<body>
    @*@Html.Partial("_LogOnPartial")*@
    <div id="header">
        <div class="navbar">
            <div class="wrapper">
                <a class="home" href="@Url.Action("Index", "Home")">Mvc.Web</a>
                <ul class="nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Weather", "Weather", "Home")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container">
        <div id="nav-section">
            <div class="carcass">
                <ul class="nav nav-list">
                    <li class="nav-header">Projects</li>
                    <li><a id="smiles">Awesome</a></li>
                    <li><a id="">LOL</a></li>
                    <li><a id="">Bash</a></li>
                </ul>
            </div>
        </div>
        <div id="news-section">
            <div class="carcass">
                <div class="container">
                    @RenderBody()
                </div>
            </div>
        </div>
        <div id="aside-section">
            <div class="carcass">
                <h2>
                    Bash</h2>
                <p>
                    &lt;erno&gt; hm. I've lost a machine.. literally _lost_. it responds to ping, it
                    works completely, I just can't figure out where in my apartment it is.
                </p>
                <p>
                    <a href="#">View details »</a>
                </p>
            </div>
        </div>
    </div>
    <footer>
        <div class="carcass">
            <p>
                &copy;травствуйте
            </p>
        </div>
    </footer>
</body>
</html>